/* pages/result/result.wxss */
page{
  background:#FFB6C1;
}
.bename{
  height: 100%;
  padding: 0 30rpx;
  margin-top: 40rpx;
  text-align: center;
}
.list{
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-left: 105rpx;
}
.list-change{
  display: flex;
  flex-direction: row;
  align-items: center;
}
.change{
  margin-left: 200rpx;
  box-shadow: 5rpx 5rpx 5rpx 5rpx #545454;
  border-radius: 100%;
  height: 50rpx;
}
.change image{
  width: 50rpx;
  height: 50rpx;
}
.text{
  color: #fff;
  font-size: 28rpx;
  margin-left: 20rpx;
}
.bename-list{
  margin-left: 34rpx;
}
.bename-title{
  /* padding-top: 34rpx; */
  padding-bottom: 10rpx;
  font-size: 44rpx;
  font-weight: bold;
  color: #ff73a3;
  text-align: center;
}
.bename-item{
  display: inline-block;
  width: 33%;
  height: 70rpx;
  line-height: 70rpx;
  padding-left: 10%;
  color: #fff;
  font-size: 30rpx;
  text-align: left;
  /* border: 1px solid #000; */
  margin: 10rpx 10rpx;
  border-radius: 20rpx;
  box-shadow: 5rpx 5rpx 5rpx 5rpx #545454;
}
.btn-bename{
  margin-top: 30rpx;
  color: #fff;
  background-color: #ff7396;
  font-size: 32rpx;
  border: 4rpx solid #ffccd9;
  border-radius: 20rpx;
}